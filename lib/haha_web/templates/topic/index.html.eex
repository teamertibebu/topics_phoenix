<h2>Topics</h2>

<ul class="collection">
  <%= for topic <- @topics do %>

  <li class="collection-item">
   <%= topic.title %>
  </li>
  <% end %>
</ul>

<%# <button type="text" >Create New Topic</button> %>
<%= button("Create New Topic", to: Routes.topic_path(@conn, :new), method: :get, class: "btn") %>
